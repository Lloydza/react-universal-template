{
  "setupFiles": [
    "./tests/setupTests.ts"
  ],
  "moduleFileExtensions": [
    "js",
    "ts",
    "tsx"
  ],
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/mocks/fileTransformer.ts",
    "\\.(css|less|scss)$": "<rootDir>/tests/mocks/styleMock.ts"
  },
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "moduleDirectories": ["node_modules", "src"]
}
