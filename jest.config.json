{
  "setupFilesAfterEnv": ["./src/test/setupTests.ts"],
  "moduleFileExtensions": ["js", "ts", "tsx"],
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/test/mocks/fileTransformer.ts",
    "\\.(css)$": "<rootDir>/src/test/mocks/styleMock.ts"
  },
  "transform": {
    "^.+\\.(ts|tsx)?$": "ts-jest"
  },
  "moduleDirectories": ["node_modules", "src"]
}
