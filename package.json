{
  "name": "react-framework",
  "version": "1.0.0",
  "description": "A framework for React applications",
  "author": "Lloyd Gray",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lloydza/react-universal-template/tree/client-side-rendering"
  },
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production webpack -p --config webpack/client.prod.js && cp -rT static dist/static",
    "start": "node src/server/server.prod.js",
    "prod": "npm run clean && cross-env ENVIRONMENT_LEVEL=4 IS_LOCAL=true NODE_ENV=development node src/server/server.dev.js",
    "uat": "npm run clean && cross-env ENVIRONMENT_LEVEL=3 IS_LOCAL=true NODE_ENV=development node src/server/server.dev.js",
    "dev": "npm run clean && cross-env ENVIRONMENT_LEVEL=2 IS_LOCAL=true NODE_ENV=development node src/server/server.dev.js",
    "local": "npm run clean && cross-env ENVIRONMENT_LEVEL=1 IS_LOCAL=true NODE_ENV=development node src/server/server.dev.js",
    "clean": "rimraf dist",
    "lint": "eslint --ext .js,.jsx src tests --fix",
    "test": "jest"
  },
  "jest": {
    "setupFiles": [
      "./tests/setupTests.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/mocks/fileTransformer.js",
      "\\.(css|less|scss)$": "<rootDir>/tests/mocks/styleMock.js"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.5.5",
    "@svgr/webpack": "^4.3.2",
    "history": "^4.9.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-helmet": "^4.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-webpack-dev-middleware": "^2.0.2",
    "koa-webpack-hot-middleware": "^1.0.3",
    "lodash": "^4.17.15",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.11",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "webpack": "^4.39.1",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-fetch-mock": "^2.1.2",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.2.0",
    "webpack-cli": "^3.3.6",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
