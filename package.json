{
  "name": "react-universal-template",
  "version": "0.0.1",
  "description": "A template for universal react websites",
  "main": "build/index.js",
  "scripts": {
    "postinstall": "webpack -p --config webpack.config.client.prod.js && webpack -p --config webpack.config.server.js",
    "start": "node build/server.js",
    "dev": "webpack -d --config webpack.config.client.dev.js --watch",
    "build-dev-server": "webpack -d --config webpack.config.server.js --watch",
    "run-dev-server": "nodemon build/server.js  --watch build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Lloydza/react-universal-template.git"
  },
  "author": "Lloyd Gray",
  "homepage": "https://github.com/Lloydza/react-universal-template",
  "dependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-react": "^7.0.0-beta.54",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.3",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-node-externals": "^1.7.2",
    "write-file-webpack-plugin": "^4.3.2"
  }
}
