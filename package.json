  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production webpack -p --config webpack/client.prod.js && webpack -p --config webpack/server.prod.js && cp -rT static dist/static",
    "start": "cross-env NODE_ENV=production node dist/server/main.js",
    "dev": "npm run clean && cross-env ENVIRONMENT_LEVEL=2 IS_LOCAL=true NODE_ENV=development ts-node-dev --respawn --transpileOnly -r tsconfig-paths/register src/server/server.dev.ts",
    "clean": "rimraf dist",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src --fix",
    "test": "jest --config jest.config.json --watch"
  },
