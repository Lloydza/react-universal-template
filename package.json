{
  "name": "react-framework",
  "version": "1.0.0",
  "description": "A framework for React applications",
  "author": "Lloyd Gray",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lloydza/react-universal-template/tree/client-side-rendering"
  },
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production webpack -p --config webpack/client.prod.js && webpack -p --config webpack/server.prod.js && cp -rT static dist/static",
    "start": "cross-env NODE_ENV=production node dist/server/main.js",
    "prod": "npm run clean && cross-env ENVIRONMENT_LEVEL=4 IS_LOCAL=true NODE_ENV=development node src/server/server.dev.js",
    "uat": "npm run clean && cross-env ENVIRONMENT_LEVEL=3 IS_LOCAL=true NODE_ENV=development node src/server/server.dev.js",
    "dev": "npm run clean && cross-env ENVIRONMENT_LEVEL=2 IS_LOCAL=true NODE_ENV=development node src/server/server.dev.js",
    "local": "npm run clean && cross-env ENVIRONMENT_LEVEL=1 IS_LOCAL=true NODE_ENV=development node src/server/server.dev.js",
    "clean": "rimraf dist",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src tests --fix",
    "test": "jest --config jest.config.json --watch"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@babel/runtime": "^7.7.2",
    "@svgr/webpack": "^4.3.3",
    "history": "^4.10.1",
