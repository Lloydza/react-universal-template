{
  "name": "react-universal-template",
  "version": "0.0.1",
  "description": "A template for universal react websites",
  "main": "build/index.js",
  "scripts": {
    "postinstall": "webpack -p --config webpack.config.client.prod.js && webpack -p --config webpack.config.server.js",
    "start": "node build/server.js",
    "dev": "webpack -d --config webpack.config.client.dev.js --watch",
    "build-dev-server": "webpack -d --config webpack.config.server.js --watch",
    "run-dev-server": "nodemon build/server.js  --watch build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Lloydza/react-universal-template.git"
  },
  "author": "Lloyd Gray",
  "homepage": "https://github.com/Lloydza/react-universal-template",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "compression": "^1.7.1",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "isomorphic-fetch": "^2.2.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "preact": "^8.2.6",
    "preact-compat": "^3.17.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "webpack": "^3.8.1",
    "webpack-node-externals": "^1.6.0"
  }
}
