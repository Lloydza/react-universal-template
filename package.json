{
  "name": "react-universal-template",
  "version": "0.0.1",
  "description": "A template for universal react websites",
  "repository": {
    "type": "git",
    "url": "git@github.com:Lloydza/react-universal-template.git"
  },
  "author": "Lloyd Gray",
  "license": "MIT",
  "homepage": "https://github.com/Lloydza/react-universal-template",
  "scripts": {
    "postinstall": "npm run clean && cross-env NODE_ENV=production node src/server/build.prod.js && cp -r public/* dist/static",
    "start": "node src/server/index.prod.js",
    "dev": "npm run clean && cross-env NODE_ENV=development node src/server/index.dev.js",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.js src/**/*.jsx --fix",
    "test": "jest"
  },
  "dependencies": {
    "compression": "^1.7.3",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-universal-component": "^3.0.3",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "webpack": "^4.19.1",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-flush-chunks": "^2.0.1",
    "webpack-hot-middleware": "^2.24.0",
    "webpack-hot-server-middleware": "^0.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-universal-import": "^3.0.2",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "extract-css-chunks-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^23.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "rimraf": "^2.6.2",
    "stats-webpack-plugin": "^0.7.0",
    "write-file-webpack-plugin": "^4.4.0"
  }
}
