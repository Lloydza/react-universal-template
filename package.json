{
  "name": "tray-react-app-boilerplate",
  "version": "1.0.0",
  "description": "A framework for building React applications",
  "author": "Tray.io",
  "private": true,
  "repository": {
    "type": "git"
  },
  "engines": {
    "node": "^10.16.3",
    "npm": "^6.9.0"
  },
  "scripts": {
    "commit": "git cz",
    "build:prod": "yarn run clean && cross-env NODE_ENV=production webpack && cp -rT static dist/static",
    "start:prod": "cross-env NODE_ENV=production node --experimental-modules src/server/server.prod.mjs",
    "prod": "yarn run build:prod && yarn run start:prod",
    "start": "yarn run clean && cross-env ENVIRONMENT_LEVEL=DEV IS_LOCAL=true NODE_ENV=development ts-node-dev --no-deprecation --respawn --transpileOnly -r tsconfig-paths/register src/server/server.dev.ts",
    "clean": "rimraf dist",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src --fix",
    "lint-style": "stylelint src/**/*.css",
    "test": "jest --config jest.config.json --watch",
    "storybook": "NODE_PATH=src/app start-storybook -p 9001 -c .storybook"
  },
  "config": {
    "commitizen": {
      "path": "@trayio/cz-conventional-tray"
    }
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@svgr/webpack": "^4.3.3",
    "@types/koa": "^2.0.52",
    "apollo-cache": "^1.3.2",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-utilities": "^1.3.2",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "history": "^4.10.1",
    "immer": "^5.0.0",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-webpack-dev-middleware": "^2.0.2",
    "koa-webpack-hot-middleware": "^1.0.3",
    "lodash": "^4.17.15",
    "react": "^16.11.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.17",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "webpack": "^4.41.2",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "devDependencies": {
    "@storybook/addon-backgrounds": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/cli": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@trayio/cz-conventional-tray": "^1.0.2",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.146",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "commitizen": "^4.0.3",
    "commitlint": "^8.2.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-color-function": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-less-vars": "^1.2.5",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.3",
    "postcss-nested": "^4.2.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "stylelint": "^12.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack-cli": "^3.3.10",
    "webpackbar": "^4.0.0",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
